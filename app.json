{
    "name": "phoenix-trello-987",
    "scripts": {
      "postdeploy": "mix ecto.migrate"
    },
    "env": {
        "DATABASE_URL": {
          "description": "The value is generated by heroku, so required is false",
          "required": false
        },
        "NPM_CONFIG_PRODUCTION": "false",
        "SECRET_KEY_BASE": {
          "generator": "secret"
        },
        "GUARDIAN_SECRET_KEY": {
          "generator": "secret"
        }
    },
    "addons": [
      {
        "plan": "heroku-postgresql",
        "options": {
          "version": "9.5"
        }
      }
    ],
    "buildpacks": [
        {
            "url": "https://github.com/ddollar/heroku-buildpack-multi"
        }
    ],
    "success_url": "/"
}
